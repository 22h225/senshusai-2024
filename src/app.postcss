@tailwind base;
@tailwind components;
@tailwind utilities;
@tailwind variants;

html,
body {
	@apply h-full overflow-hidden;
}

.kaisei-harunoumi-regular {
	font-family: 'Kaisei HarunoUmi', serif;
	font-weight: 400;
	font-style: normal;
}

.kaisei-harunoumi-medium {
	font-family: 'Kaisei HarunoUmi', serif;
	font-weight: 500;
	font-style: normal;
}

.kaisei-harunoumi-bold {
	font-family: 'Kaisei HarunoUmi', serif;
	font-weight: 700;
	font-style: normal;
}
